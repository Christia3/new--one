function age(yearofBirth){
    let currentYear = 2025
    let age = currentYear - yearofBirth;
    if(age < 13){
    console.log ( "child")
}else if (age >= 13 && age <= 19){
    console.log ( "teenager")
}
else if (age >= 20 && age <=59 ){
    console.log ( "adult")
}
else {
    console.log ("senior")
}
}
 age(2000)
